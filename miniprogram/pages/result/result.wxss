/* miniprogram/pages/result/result.wxss */
.result {
  padding: 28rpx;
  color: #333;
  font-size: 30rpx;
}
.result .title {
  font-size: 35rpx;
  font-weight: bold;
  padding: 30rpx 0 20rpx;
}
.head {
  text-align: center;
  font-weight: bold;
  font-size: 45rpx;
}
.sub-head {
  text-align: center;
  font-size: 30rpx;
  color: #666;
  padding: 10rpx 0 30rpx;
}
.content {
  background: #efefef;
  padding: 10rpx;
}
.content > view {
  padding: 10rpx 0;
}
.case-title {
  font-weight: bold;
  padding: 20rpx 0 10rpx;
}
.case-content-title {
  padding: 10rpx 0;
}
.case-content {
  background-color: #efefef;
  padding: 10rpx;
}